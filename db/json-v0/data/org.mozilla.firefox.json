{
    "info": {
        "id": "org.mozilla.firefox",
        "title": "Firefox",
        "endoflife_date_id": "firefox"
    },
    "binary": [
        {
            "path": "/usr/bin/firefox",
            "arguments": [
                "--version"
            ],
            "regex": "^(?<version>(?<cycle>(?<major>\\d+)).(?<minor>\\d+).(?<patch>\\d+))"
        }
    ],
    "flatpak": {
        "id": "org.mozilla.firefox",
        "regex": "(?<version>(?<cycle>(?<major>\\d+)).(?<minor>\\d+).(?<patch>\\d+))"
    }
}
